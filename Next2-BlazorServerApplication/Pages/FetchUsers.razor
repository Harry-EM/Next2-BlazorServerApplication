@page "/FetchUsers"
@inject AppDbContext DbContext

<h3>answer table</h3>

@if (Answers == null)
{
    <p>empty</p>
}
@else
{
    @foreach (var answer in Answers)
    {
        <ul>@answer.answer_id</ul>
    }
}


@code {
    private List<User> users;

    protected override async Task OnInitializedAsync()
    {
        users = await DbContext.Users.ToListAsync();
    }
}

